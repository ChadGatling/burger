<div class="container">
		<div class="jumbotron">
			<h1>Eat Da Burger!</h1>
			<h3>Come on down and get somadis burger!</h3>
			<hr>
			<a href="/" class="btn btn-danger btn-lg"><span class="glyphicon glyphicon-eye-open"></span>Search Burgers</a>
			<a href="/add" class="btn btn-danger btn-lg"><span class="glyphicon glyphicon-plus"></span>Add New Burger</a>
		</div>
		<div class="row">

			<div class="col-lg-12">

				<div class="panel panel-default">
					<div class="panel-heading">
						<h3 class="panel-title"><strong>Character Database</strong></h3>
					</div>
					<div class="panel-body" id="well-section">
				
					<!-- Content will go in here -->

						{{#each burgers}}
						<div>
							<h3>{{burger_name}}</h3>
							<a href=""></a>
						</div>
						{{/each}}

					</div>
				</div>
			</div>			
		</div>
	</div>

<script src="js/all.js" type="text/javascript"></script>

